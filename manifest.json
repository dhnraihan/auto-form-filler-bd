{
  "manifest_version": 2,
  "name": "Auto fill",
  "version": "1.1",
  "description": "Fills forms with fake data on button click",
  "permissions": ["activeTab", "storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}